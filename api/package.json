{
	"name": "api",
	"version": "1.0.0",
	"description": "",
	"scripts": {
		"build": "tsc && mkdir -p dist/assets && cp -r src/assets/fonts dist/assets/fonts",
		"watch": "tsc -w",
		"clean": "rimraf dist",
		"prestart": "npm run clean && npm run build",
		"start": "func start",
		"test": "vitest run",
		"test:watch": "vitest --watch",
		"test:coverage": "vitest run --coverage",
		"lint": "npx @biomejs/biome check src --write",
		"format": "npx @biomejs/biome format src --write"
	},
	"dependencies": {
		"@azure/functions": "^4.0.0",
		"canvas": "^3.2.0",
		"mongoose": "^9.0.1"
	},
	"devDependencies": {
		"@types/node": "^20.x",
		"@vitest/coverage-v8": "^4.0.10",
		"rimraf": "^5.0.0",
		"typescript": "^4.0.0",
		"vitest": "^4.0.3"
	},
	"main": "dist/index.js"
}
